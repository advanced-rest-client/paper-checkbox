{
  "name": "paper-checkbox",
  "version": "1.9.0",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "Anypoint version of paper-checkbox element. This meant to be private.",
  "devDependencies": {
    "bower": "^1.8.0",
    "polymer-cli": "^1.2.0"
  },
  "private": false,
  "main": [
    "paper-checkbox.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint paper-checkbox.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"paper-checkbox:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mulesoft/paper-checkbox.git"
  },
  "bugs": {
    "url": "https://github.com/mulesoft/paper-checkbox/issues",
    "email": "arc@mulesoft.com"
  }
